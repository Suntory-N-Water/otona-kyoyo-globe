{
  "name": "otona-kyoyo-globe",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "format": "biome format --write .",
    "check": "biome check .",
    "check:fix": "biome check --write .",
    "lint": "biome lint .",
    "lint:unsafe-fix": "biome lint --write --unsafe .",
    "lint:ai": "biome lint --reporter=github . 2>&1 | { grep '^::' || true; }",
    "type-check": "tsc --noEmit",
    "type-check:ai": "tsc --noEmit --pretty false",
    "ai-check": "bun run format && bun run lint:ai && bun run type-check:ai",
    "pipeline:fetch": "bun run src/pipeline/fetch-videos.ts",
    "pipeline:extract": "bun run src/pipeline/extract-locations.ts",
    "pipeline:geocode": "bun run src/pipeline/geocode.ts",
    "pipeline:validate": "bun run src/pipeline/validate-and-merge.ts",
    "pipeline": "bun run pipeline:fetch && bun run pipeline:extract && bun run pipeline:geocode && bun run pipeline:validate"
  },
  "dependencies": {
    "@google/genai": "^1.42.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.575.0",
    "radix-ui": "^1.4.3",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-globe.gl": "^2.37.0",
    "react-leaflet": "^5.0.0",
    "supercluster": "^8.0.1",
    "tailwind-merge": "^3.5.0",
    "three": "^0.183.1"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.14",
    "@tailwindcss/vite": "^4.2.0",
    "@types/leaflet": "^1.9.21",
    "@types/node": "^25.3.0",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@types/supercluster": "^7.1.3",
    "@types/three": "^0.183.1",
    "@vitejs/plugin-react-swc": "^4.2.3",
    "playwright": "^1.58.2",
    "shadcn": "^3.8.5",
    "tailwindcss": "^4.2.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.56.0",
    "vite": "^7.3.1"
  }
}
